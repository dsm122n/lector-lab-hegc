{
    "manifest_version": 3,
    "name": "lector lab",
    "description": "Base Level Extension",
    "version": "1.0",
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "index.html",
      "default_icon": "icon-1.png"
    },
    "permissions": [
      "storage",
      "activeTab",
      "scripting",
      "tabs",
      "http://*/*",
      "https://*/*",
      "file:///*/*"
    ]

  }
